{
  "name": "@textea/shared",
  "version": "0.0.6",
  "description": "Type definitions for Textea packages",
  "license": "MIT",
  "repository": "https://github.com/TexteaInc",
  "bugs": {
    "url": "https://github.com/TexteaInc/shared/issues"
  },
  "homepage": "https://github.com/TexteaInc/shared#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "doc": "typedoc --json docs/typedoc-out.json src/index.ts && node scripts/docs-build.mjs",
    "prepare": "husky install",
    "lint:check": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "changelog": "git-cliff --output CHANGELOG.md",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-angular": "^16.2.4",
    "@discordjs/ts-docgen": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "packageManager": "yarn@3.2.1"
}
